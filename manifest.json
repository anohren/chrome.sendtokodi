{
  "name": "SendToKodi",
  "description": "SendToKodi allows you to send almost any stream from Chrome to Kodi.",
  "short_name": "SendToKodi",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "http://*/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 3,
  "version": "0.0.17"
}
